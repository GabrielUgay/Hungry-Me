<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        tools:context=".MainActivity7">

        <ImageView
            android:id="@+id/back"
            android:layout_width="51dp"
            android:layout_height="55dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.077"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.06"
            app:srcCompat="@drawable/back2"
            app:tint="@color/black"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="20dp"
            app:layout_constraintTop_toBottomOf="@id/back"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:id="@+id/mangInasal"
                    android:layout_width="260dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:background="@drawable/black_opacity"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:padding="0dp">

                    <ImageView
                        android:id="@+id/inasal"
                        android:layout_width="347dp"
                        android:layout_height="142dp"
                        app:srcCompat="@drawable/inasal" />

                    <!-- #7EEFDE -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:text="Mang Inasal"
                        android:textSize="18dp"
                        android:textColor="#7EEFDE"
                        android:fontFamily="@font/inria_serif"
                        />

                    <!-- #7EEFDE -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/inder"
                        android:text="Fastfood Chain"
                        android:textColor="#7EEFDE"
                        android:textSize="12dp" />

                </LinearLayout>

                <FrameLayout
                    android:id="@+id/mangInasalCart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="90dp"
                    android:layout_marginStart="15dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/rec2405"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/shop"/>
                </FrameLayout>


            </LinearLayout>









            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="20dp"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/jollibee"
                    android:layout_width="260dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/black_opacity"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:padding="0dp">

                    <ImageView
                        android:id="@+id/jolibee"
                        android:layout_width="315dp"
                        android:layout_height="142dp"
                        app:srcCompat="@drawable/readibee" />

                    <!-- #7EEFDE -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:text="Jollibee"
                        android:textSize="18dp"
                        android:textColor="#7EEFDE"
                        android:fontFamily="@font/inria_serif"
                        />

                    <!-- #7EEFDE -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/inder"
                        android:text="Fastfood Chain"
                        android:textColor="#7EEFDE"
                        android:textSize="12dp" />

                </LinearLayout>

                <FrameLayout
                    android:id="@+id/jollibeeCart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="90dp"
                    android:layout_marginStart="15dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/rec2405"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/shop"/>
                </FrameLayout>

            </LinearLayout>










            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/greenwichLinearLayout"
                    android:layout_width="260dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="50dp"
                    android:background="@drawable/black_opacity"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:padding="0dp">

                    <ImageView
                        android:id="@+id/greenwich"
                        android:layout_width="314dp"
                        android:layout_height="140dp"
                        app:srcCompat="@drawable/greenwich" />

                    <!-- #7EEFDE -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:text="Greenwich"
                        android:textSize="18dp"
                        android:textColor="#7EEFDE"
                        android:fontFamily="@font/inria_serif"
                        />

                    <!-- #7EEFDE -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/inder"
                        android:text="Fastfood Chain"
                        android:textColor="#7EEFDE"
                        android:textSize="12dp" />

                </LinearLayout>

                <FrameLayout
                    android:id="@+id/greenwichCart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="90dp"
                    android:layout_marginStart="15dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/rec2405"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/shop"/>
                </FrameLayout>

            </LinearLayout>


        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>