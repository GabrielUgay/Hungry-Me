    <ImageView
        android:id="@+id/addressLogo"
        android:layout_width="17dp"
        android:layout_height="22dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.6825"
        app:srcCompat="@drawable/address" />

    <TextView
        android:id="@+id/logOutLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/door2"
        android:drawablePadding="4dp"
        android:fontFamily="@font/inria_serif"
        android:gravity="center"
        android:text="LOG OUT"
        android:textColor="#EED6D6"
        android:textFontWeight="700"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.972" />

